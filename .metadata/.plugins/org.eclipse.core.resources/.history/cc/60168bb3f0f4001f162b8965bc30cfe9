package Example;

public class Example2 {

	public static void main(String[] args) {

		// temp 선언 : 변수의 값을 교환할 때 사용
		int a = 10;
		int b = 20;
		int c = 30;
		System.out.println("a: " + a + "," + "b: " + b + "," + "c: " + c);
		// >> tamp 사용 전 표현
		
		int temp = c;
		b = c; c = a;
		System.out.println("a: " + a + "," + "b: " + b + "," + "c: " + c);
		// b = c 값인 30, 
	}

}
